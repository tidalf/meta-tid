+++ b/arch/arm/boot/dts/imx6qdl-curie.dtsi
@@ -89,7 +89,7 @@
 		disp_dev = "hdmi";
 		interface_pix_fmt = "RGB24";
 		mode_str ="1920x1080M@60";
